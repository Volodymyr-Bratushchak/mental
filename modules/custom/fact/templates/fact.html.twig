{#
/**
 * @file fact.html.twig
 * Default theme implementation to present Fact data.
 *
 * This template is used when viewing Fact pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_fact()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('fact') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
