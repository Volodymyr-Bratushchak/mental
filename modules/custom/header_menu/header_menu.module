<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function header_menu_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['header_menu_template'] = [
    'template' => 'header-menu-template',
    'variables' => [
      'menu_items' => NULL,
    ],
  ];
  return $theme;
}

