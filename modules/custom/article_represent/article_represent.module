<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function article_represent_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['article_represent_template'] = [
    'template' => 'article-represent-template',
    'variables' => [
      'paragraph_data' => NULL,
    ],
  ];
  return $theme;
}



