<?php
/**
 * Implements hook_theme() to add the template definition.
 **/

function footer_floss_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['footer_floss_template'] = [
    'template' => 'footer-floss-template',
    'variables' => [
      'test' => NULL,
    ],
  ];
  return $theme;
}

