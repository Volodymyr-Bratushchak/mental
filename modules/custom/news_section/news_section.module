<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function news_section_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['news_section_template'] = [
    'template' => 'news-section-template',
    'variables' => [
      'sections' => NULL,
    ],
  ];
  return $theme;
}

