<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function footer_menu_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['footer_menu_template'] = [
    'template' => 'footer-menu-template',
    'variables' => [
      'menu_items' => NULL,
    ],
  ];
  return $theme;
}

